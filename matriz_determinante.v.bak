module matriz_determinante #(parameter WIDTH=8)(
	input signed [WIDTH-1:0] A [0:2][0:2],
	output reg signed [(3*WIDTH):0] det
);
	always @(*) begin
		det = (A[0][0]*(A[1][1]*A[2][2]-A[1][2]*A[2][1])) - 
			  (A[0][1]*(A[1][0]*A[2][2]-A[1][2]*A[2][0])) +
			  (A[0][2]*(A[1][0]*A[2][1]-A[1][1]*A[2][0]));
	end
endmodule
