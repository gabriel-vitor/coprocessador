module matriz_multiplicacao #(parameter N=3, WIDTH=8)(
	input signed [WIDTH-1:0] A [0:N-1][0:N-1],
	input signed [WIDTH-1:0] B [0:N-1][0:N-1],
	output reg signed [(2*WIDTH)+2:0] Resultado [0:N-1][0:N-1]
);
	integer i, j, k;
	always @(*) begin
		for(i=0;i<N;i=i+1) begin
			for(j=0;j<N;j=j+1) begin
				Resultado[i][j] = 0;
				for(k=0;k<N;k=k+1)
					Resultado[i][j] = Resultado[i][j] + (A[i][k]*B[k][j]);
			end
		end
	end
endmodule
